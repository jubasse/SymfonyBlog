{% extends '::base.html.twig' %}
{% block content %}

    {% for post in posts %}
        <div>
            <h1>{{ post.title }}</h1>
            <p>{{ post.content }}</p>
            <em>last update : {{ post.updatedAt|date }}</em>
        </div>
    {% endfor %}
    <br>
    <br>
    <a href="{{ path(pathName) }}">Voir les {{ invert }} post</a>
    <hr>
{% endblock %}